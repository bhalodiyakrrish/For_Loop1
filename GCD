def gcd(a, b):
    small = min(a, b)
    ans = 1
    for i in range(1, small + 1):
        if a % i == 0 and b % i == 0:
            ans = i
    return ans  
if __name__ == '__main__':
    t=int(input())
    for _ in range(t):
        A=int(input())
        B=int(input()) 
        ans = gcd(A,B)
        print(ans)

# Input: 
# 6
# 9
# Output: 
# 3
