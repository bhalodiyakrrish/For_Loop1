def prime(n):
    if n<=1:
        return False
    for i in range(2,n):
        if n%i==0:
            return False
    return True
if __name__ == '__main__':
    t=int(input())
    for _ in range(t):
        n=int(input())
        ans = prime(n)
        print(ans)

# Input: 
# 17
# Output: 
# True
