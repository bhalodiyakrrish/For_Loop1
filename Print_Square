def square(s):
    for i in range(s):
        if i == 0 :       
            print("* "*s) 
        elif i<s-1 :
            print("* "," "*(s-2),"*")
        else:
            print("* "*s)
if __name__ == '__main__':
    t=int(input())
    for _ in range(t):
        s=int(input())
        square(s)
        
# Input: 
# 4
# Output: 
# * * * *
# *     *
# *     *
# * * * *
